<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.sytes.roneymaia.agenda">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastrarLoginActivity"
            android:label="@string/cadastrarlogin" />
        <activity
            android:name=".AgendaActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ContatosActivity"
            android:label="@string/contatos" />
        <activity
            android:name=".CompromissosActivity"
            android:label="@string/compromissos" />
        <activity
            android:name=".NovoContatoActivity"
            android:label="@string/novocontato" />
        <activity
            android:name=".EditarContatoActivity"
            android:label="@string/contato" />
        <activity android:name=".NovoCompromissoActivity"
            android:label="@string/compromisso" />
    </application>

</manifest>